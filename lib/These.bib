@article{Gao/TIE15,
	title = {A {Survey} of {Fault} {Diagnosis} and {Fault}-{Tolerant} {Techniques}—{Part} {I}: {Fault} {Diagnosis} {With} {Model}-{Based} and {Signal}-{Based} {Approaches}},
